# Cppkg Base Dependency Downloader
cmake_minimum_required(VERSION 3.6)
project(serdepp-0.1.3-install)

set(CPPM_VERSION ${CPPM_VERSION})
include(${CMAKE_CURRENT_SOURCE_DIR}/cmake/cppm_loader.cmake)
download_package(serdepp 0.1.3  URL https://github.com/injae/serdepp/archive/refs/tags/v0.1.3.zip  TYPE lib CMAKE_ARGS ${CMAKE_ARGS} )

